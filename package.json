{
  "name": "wedding-site",
  "version": "1.0.0",
  "description": "Wedding site converted from Elm to Node.js",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "migrate": "knex migrate:latest",
    "server": "nodemon --exec ts-node server/index.ts",
    "build": "tsc && cd client && npm run build",
    "client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/basic-auth": "^1.1.8",
    "basic-auth": "^2.0.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "koa": "^3.1.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^14.0.0",
    "koa-swagger-decorator": "^1.8.7",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "pg": "^8.16.3",
    "socket.io": "^4.6.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/koa": "^3.0.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.9",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.9.2",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
